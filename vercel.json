{
  "version": 2,
  "builds": [
    {
      "src": "vercel/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/vercel/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "cache-control": "s-maxage=1, stale-while-revalidate"
      }
    }
  ]
}
